{
  "manifest_version": 2,
  "name": "Gopher Search Extension",
  "version": "1.0",
  "content_scripts": [
  {
	"matches": ["*://*/search*"],
    "js": ["jquery-3.1.1.min.js", "content.js"],
    "runat": "document_start"
  }
],
  "browser_action": {
  "default_icon": "gopher.png"
},
"background": {
    "persistent": true,
    "scripts": ["background.js"]
},

    "permissions": [
        "storage", 
        "tabs"
    ]

}
